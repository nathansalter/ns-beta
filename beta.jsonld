{
  "@context": [
    "https://openactive.io/extension",
    {
      "beta": "https://openactive.io/ns-beta#"
    }
  ],
  "@graph": [
    {
      "id": "beta:formattedDescription",
      "type": "Property",
      "label": "formattedDescription",
      "comment": "Sometimes a description is stored with formatting (e.g. href, bold, italics, embedded YouTube videos). This formatting can be useful for data consumers.",
      "domainIncludes": [
        "schema:Event",
        "schema:Place"
      ],
      "rangeIncludes": [
        "schema:Text"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/ns-beta/issues/2"
    },
    {
      "id": "beta:distance",
      "type": "Property",
      "label": "distance",
      "comment": "The distance of a run, cycle or other activity. Must also include units.",
      "domainIncludes": [
        "schema:Event"
      ],
      "rangeIncludes": [
        "schema:QuantitativeValue"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/ns-beta/issues/3"
    },
    {
      "id": "beta:availability",
      "type": "Property",
      "label": "availability",
      "comment": "For data publishers not wishing to disclose the granular availability of their sessions openly.",
      "domainIncludes": [
        "schema:Event"
      ],
      "rangeIncludes": [
        "schema:Text"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/ns-beta/issues/9"
    },
    {
      "id": "beta:attendeeCount",
      "type": "Property",
      "label": "attendeeCount",
      "comment": "For events that have an unlimited number of tickets, captures the number of attendees (actual attendance).",
      "domainIncludes": [
        "schema:Event"
      ],
      "rangeIncludes": [
        "schema:Integer"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/ns-beta/issues/12"
    },
    {
      "id": "beta:registrationCount",
      "type": "Property",
      "label": "registrationCount",
      "comment": "For events that have an unlimited number of tickets, captures the number of registrations (intention to attend).",
      "domainIncludes": [
        "schema:Event"
      ],
      "rangeIncludes": [
        "schema:Integer"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/ns-beta/issues/13"
    },
    {
      "id": "beta:course",
      "type": "Property",
      "label": "course",
      "comment": "This course for which this is an offering.",
      "domainIncludes": [
        "schema:CourseInstance"
      ],
      "rangeIncludes": [
        "schema:Course"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/164"
    },
    {
      "id": "beta:logo",
      "type": "Property",
      "label": "logo",
      "comment": "An associated logo for a course.",
      "domainIncludes": [
        "schema:Course"
      ],
      "rangeIncludes": [
        "schema:ImageObject"
      ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/164"
    },
    {
      "id": "https://openactive.io/ns-beta#Member",
      "type": "schema:BusinessEntityType",
      "label": "Member",
      "comment": "Indicates that a customer (eligableCustomerType) is a member of the business.",
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/80"
    },
    {
      "id": "beta:availableChannel",
      "type": "Property",
      "label": "availableChannel",
      "comment": "The channels through which a booking can be made.",
      "domainIncludes": [
        "schema:Offer"
      ],
      "rangeIncludes": [
        "beta:AvailableChannelType"
      ],
      "isArray": true,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/161"
    },
    {
      "id": "beta:AvailableChannelType",
      "type": "Class",
      "label": "AvailableChannelType",
      "comment": "An enumeration of channels through which a booking can be made.",
      "subClassOf": [
        "schema:Enumeration"
      ],
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/161"
    },
    {
      "id": "https://openactive.io/ns-beta#TelephoneAdvanceBooking",
      "type": "beta:AvailableChannelType",
      "label": "TelephoneAdvanceBooking",
      "comment": "Bookings can be made but not paid for in advance by telephone.",
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/161"
    },
    {
      "id": "https://openactive.io/ns-beta#TelephonePrepayment",
      "type": "beta:AvailableChannelType",
      "label": "TelephonePrepayment",
      "comment": "Bookings can be made and paid for in advance by telephone.",
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/161"
    },
    {
      "id": "https://openactive.io/ns-beta#OnlinePrepayment",
      "type": "beta:AvailableChannelType",
      "label": "OnlinePrepayment",
      "comment": "Bookings can be made and paid for online.",
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/161"
    },
    {
      "id": "https://openactive.io/ns-beta#OpenBookingPrepayment",
      "type": "beta:AvailableChannelType",
      "label": "OpenBookingPrepayment",
      "comment": "Bookings can be made via the Open Booking API.",
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/161"
    },
    {
      "id": "beta:sportsActivityLocation",
      "type": "Property",
      "label": "sportsActivityLocation",
      "comment": "The specific array of SportsActivityLocation related to the FacilityUse, usually within the location.",
      "domainIncludes": [
        "oa:FacilityUse"
      ],
      "rangeIncludes": [
        "schema:SportsActivityLocation"
      ],
      "isArray": true,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/110"
    },
    {
      "id": "beta:isWheelchairAccessible",
      "type": "Property",
      "label": "isWheelchairAccessible",
      "comment": "A property that details whether the event is suitable for wheelchair access. Placed on Event as this field could be used to detail whether the Event is suitable, as well as the Place.",
      "domainIncludes": [ "schema:Event" ],
      "rangeIncludes": [ "schema:Boolean" ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/166"
    },
    {
      "id": "beta:estimatedDuration",
      "type": "Property",
      "label": "estimatedDuration",
      "comment": "A property that allows an Event duration to be represented as a range (e.g. 0-30mins, 30-60mins, 60-90mins, 90+).",
      "domainIncludes": [ "schema:Event" ],
      "rangeIncludes": [ "schema:QuantitativeValue" ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/201"
    },
    {
      "id": "beta:ConceptCollection",
      "type": "Class",
      "label": "ConceptCollection",
      "comment": "A SKOS Collection for use with SKOS ConceptScheme",
      "subClassOf": [
        "skos:Collection",
        "schema:CreativeWork"
      ],
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/203"
    },
    {
      "id": "beta:timeZone",
      "type": "Property",
      "label": "timeZone",
      "comment": "The time zone used to generate occurrences, same as iCal TZID. E.g. 'Europe/London'.",
      "domainIncludes": [ "pending:Schedule" ],
      "rangeIncludes": [ "schema:Text" ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/197"
    },
    {
      "id": "beta:video",
      "type": "Property",
      "label": "video",
      "comment": "An related video object.",
      "domainIncludes": [ "schema:Event", "oa:FacilityUse", "schema:Brand"  ],
      "rangeIncludes": [ "schema:VideoObject" ],
      "isArray": true,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/88"
    },
    {
      "id": "beta:sportsActivityLocation",
      "type": "Property",
      "label": "sportsActivityLocation",
      "comment": "Internal location of the event, e.g. Court 1",
      "domainIncludes": [ "schema:Event"  ],
      "rangeIncludes": [ "schema:SportsActivityLocation" ],
      "isArray": true,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/110"
    },
    {
      "id": "beta:offerValidityPeriod",
      "type": "Property",
      "label": "offerValidityPeriod",
      "comment": "Duration before the event for which the associated Offers are valid",
      "domainIncludes": [ "schema:Event", "oa:FacilityUse" ],
      "rangeIncludes": [ "schema:Duration" ],
      "isArray": false,
      "githubIssue": "https://github.com/openactive/modelling-opportunity-data/issues/204"
    }
  ]
}
